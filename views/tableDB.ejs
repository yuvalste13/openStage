
<table class = "table">
  <thead>
    <tr>
      <th scope="col">_id</th>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
      <th scope="col">Day</th>
      <th scope="col">Start Time</th>
      <th scope="col">Image</th>
      <th scope="col">Rank</th>
      <th scope="col">Reviews</th>
    </tr>
  </thead>
  <tbody>


  <% for(let i=0 ; i < Stages.length ; i++){%>
    <tr>
      <td scope="row"><%= Stages[i]._id %></td>
      <td><%= Stages[i].Name %></td>
      <td><%= Stages[i].Address.City %> , <%= Stages[i].Address.Street%> <%= Stages[i].Address.Number%></td>
      <td><%= Stages[i].Day %></td>
      <td><%= Stages[i].startTime %></td>
      <td><%= Stages[i].Img %></td>
      <td><%= Stages[i].rank %></td>
      <td><%= Stages[i].rev %></td>
    </tr>

  <%}%>


  </tbody>
</table>
