<h2 class="Open">במות פתוחות</h2>
<hr>


<div class="row">

  <% StageList.forEach(function(stage){ %>

  <div class="col-lg-5 card shadow-lg text-center rounded-5 text-white bg-dark overflow-hidden stage-day card-cover <%= stage.Day %>" style="width: 18rem;">
    <img src="<%= stage.Img %>" class="card-img-top" alt="...">
    <div class="card-body">
      <h3 class="card-title"> <%= stage.Name %> </h3>
      <p class="card-text"> <%= stage.Location %> </p>
      <p class="card-text"> <%= stage.Day %> </p>
      <p class="card-text"></p>

    </div>
  </div>

  <% }); %>

</div>




<script src="scripts/index.js" charset="utf-8"></script>
